// A set of LCM types to make it easy to serialize Eigen types and send them over LCM.
// These are hand-written types that aren't included in the generated eigen_lcm.lcm.

package eigen_lcm;

struct Matrix24d {
  double data[8];
};

struct Matrix34d {
  double data[12];
};

struct Matrix54d {
  double data[20];
};

struct Matrix43f {
  float data[12];
};

struct Matrix68f {
  float data[48];
};
